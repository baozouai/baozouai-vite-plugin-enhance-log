{
  "name": "vite-plugin-enhance-log",
  "version": "0.2.1-beta.0",
  "packageManager": "pnpm@8.3.1",
  "description": "A vite plugin to log filename, log argument name and separator",
  "author": "baozouai <baozouai@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/baozouai/vite-plugin-enhance-log",
  "repository": {
    "type": "git",
    "url": "https://github.com/baozouai/vite-plugin-enhance-log.git"
  },
  "bugs": {
    "url": "https://github.com/baozouai/vite-plugin-enhance-log/issues"
  },
  "keywords": [
    "vite",
    "plugin",
    "enhance log",
    "vite-plugin",
    "add log argument name",
    "Add the file name of the log file",
    "add separator for every argument"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint . --fix",
    "build": "unbuild",
    "test": "vitest",
    "tag": "bumpp",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "npm publish",
    "prepare": "husky install",
    "play": "pnpm --filter vue dev",
    "play:react": "pnpm --filter react dev"
  },
  "peerDependencies": {
    "vite": "^2.9.0 || ^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.21.4",
    "@babel/parser": "^7.22.5",
    "@babel/plugin-syntax-typescript": "^7.21.4",
    "@babel/traverse": "7.21.4",
    "source-map": "^0.7.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@babel/types": "^7.21.4",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.20.1",
    "@types/node": "^18.15.13",
    "@vitest/coverage-c8": "^0.30.1",
    "bumpp": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "pnpm": "^8.3.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^7.1.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vite-plugin-inspect": "^0.7.16",
    "vitest": "^0.30.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --fix"
  }
}
